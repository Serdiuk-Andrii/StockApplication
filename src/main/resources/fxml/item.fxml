<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="item_pane" onMouseReleased="#click" prefHeight="200.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Client.Model.Item">
   <children>
      <VBox id="item_vbox" layoutX="50.0" prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox id="item_name_hbox" alignment="CENTER" prefHeight="62.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
               <children>
                  <Label id="itemProductName" fx:id="itemProductName" text="Product name" />
               </children>
            </HBox>
            <HBox id="item_cost_hbox" alignment="CENTER" prefHeight="48.0" prefWidth="200.0">
               <children>
                  <Label id="itemCost" fx:id="itemCost" text="Total cost: " />
               </children>
            </HBox>
            <HBox id="item_cost_hbox" alignment="CENTER" prefHeight="60.0" prefWidth="200.0">
               <children>
                  <Label id="itemAmount" fx:id="itemAmount" text="Amount of products" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
